{
    "extends": ["@parcel/config-default"],
    "reporters":  ["...", "parcel-reporter-static-files-copy", "@parcel/reporter-sourcemap-visualiser"],
    "transformers": {
        "*.{ts,tsx}": ["parcel-transformer-tsc-sourcemaps"]
    },
    "compressors": {
        "*.{html,css,js,svg,map}": [
              "...",
              "@parcel/compressor-gzip",
              "@parcel/compressor-brotli"
        ]
    }
}
